{
    "name": "typicms/base",
    "description": "Multilingual CMS build with Laravel 5.0",
    "keywords": ["cms", "multilingual", "laravel", "typi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "laravel/framework": "~5.0.0",
        "barryvdh/laravel-debugbar": "~2.0",
        "cartalyst/sentry": "dev-feature/laravel-5",
        "cviebrock/image-validator": "~1.0",
        "edvinaskrucas/notification": "dev-master",
        "laracasts/utilities": "~2.0",
        "msurguy/honeypot": "dev-master",
        "typicms/core": "~1.8",
        "typicms/events": "~1.8",
        "typicms/news": "~1.8",
        "typicms/projects": "~1.8",
        "typicms/partners": "~1.8",
        "typicms/categories": "~1.8",
        "typicms/contacts": "~1.8",
        "typicms/places": "~1.8",
        "pingpong/menus": "2.*@dev",
        "patchwork/utf8": "~1.2",
        "bkwld/croppa": "dev-master",
        "typicms/translatablebootforms": "~2.0.0",
        "guzzlehttp/guzzle": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/WirelessAnalytics/notification"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
