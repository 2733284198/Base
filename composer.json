{
    "name": "typicms/base",
    "description": "Multilingual CMS build with Laravel 5.0",
    "keywords": ["cms", "multilingual", "laravel", "typi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "laravel/framework": "~5.0.23",
        "laravelcollective/html": "~5.0",
        "barryvdh/laravel-debugbar": "~2.0",
        "cartalyst/sentry": "dev-feature/laravel-5",
        "cviebrock/image-validator": "~2.0.0",
        "edvinaskrucas/notification": "~5.0.0",
        "laracasts/utilities": "~2.0",
        "msurguy/honeypot": "~0.3.2",
        "patchwork/utf8": "~1.2",
        "bkwld/croppa": "~3.6.1",
        "guzzlehttp/guzzle": "~4.0",
        "maatwebsite/laravel-sidebar": "~1.0.0",
        "typicms/nestablecollection": "~1.1.0",
        "typicms/translatablebootforms": "~2.0.2",
        "typicms/core": "~2.0.22",
        "typicms/blocks": "~2.0.8",
        "typicms/dashboard": "~2.0.5",
        "typicms/files": "~2.0.6",
        "typicms/galleries": "~2.0.8",
        "typicms/groups": "~2.0.4",
        "typicms/history": "~2.0.4",
        "typicms/menus": "~2.0.16",
        "typicms/pages": "~2.0.19",
        "typicms/settings": "~2.0.6",
        "typicms/sitemap": "~2.0.3",
        "typicms/tags": "~2.0.5",
        "typicms/translations": "~2.0.6",
        "typicms/users": "~2.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
