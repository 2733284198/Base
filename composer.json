{
    "name": "sdebacker/typicms",
    "description": "Multilingual CMS build with Laravel 4.2",
    "keywords": ["CMS", "multilingual", "Laravel", "Typi"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel De Backer",
            "email": "samuel@typidesign.be",
            "role": "Developer"
        }
    ],
    "type": "project",
    "require": {
        "bkwld/croppa": "~3.4.2",
        "cartalyst/sentry": "~2.1.4",
        "davejamesmiller/laravel-breadcrumbs": "~2.2.3",
        "edvinaskrucas/notification": "~3.0",
        "guzzlehttp/guzzle": "~4.2.3",
        "jwage/easy-csv": "~0.0.2",
        "laravel/framework": "~4.2.11",
        "thomaswelton/laravel-gravatar": "~0.1",
        "cviebrock/image-validator": "~1.0.4",
        "jenssegers/rollbar": "~1.3",
        "laracasts/utilities": "~1.0",
        "vlucas/phpdotenv": "~1.0",
        "typicms/pages": "dev-master",
        "typicms/menus": "dev-master",
        "typicms/settings": "dev-master",
        "typicms/dashboard": "dev-master",
        "typicms/users": "dev-master",
        "typicms/groups": "dev-master",
        "typicms/history": "dev-master",
        "typicms/sitemap": "dev-master",
        "typicms/translations": "dev-master",
        "typicms/tags": "dev-master",
        "typicms/translatablebootforms": "~1.1.0"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-4": {
            "TypiCMS\\": "app/TypiCMS"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
